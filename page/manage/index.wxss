/* page/search/index.wxss */
.wxLogin{
    width: 50%;
    font-size: 18px;
  }
.container{
  background: #f1f1f1;
}
.now{
  display: flex;
  align-items: center;
  margin:0 10px;
  background: #ddd;
  border-radius: 5px;
}
.list-h {
  position: relative;
  font-size: 14px;
  background: #fff;
  color:#666;
  margin-top:10px;
  /* border-bottom: 1px solid #ddd; */
}

.name-h{
  font-size:15px;color:#000;width: 100px;display: inline-block;
}
.address-h{
  /* display: inline-block; */
  padding-right:5px;
}
.icon-h{
  position: absolute;
  right:10px;
  top:10px;
}
.title-now{
  height: 40px;
  width: 100%;
  background: #fff;
  margin-top:10px;
  margin-bottom:10px;
  padding:0 10px;
  line-height: 40px;
}
.no-order{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.no-order view{
  color:#666;
}
.list-address{
  display: flex;
  height: 30px;
  width: 100%;
  justify-content: space-between;
  align-items: center;
}
.show{
  width: 100%;
 
  padding: 10px;
  box-sizing: border-box;

}
.address{
  font-size: 15px;
  font-weight: 600;
  width:90px;
}
.delete{
  width: 25%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center
}